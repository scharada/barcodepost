<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net-net-1.0"/>
  </configSections>
  <log4net debug="true">
    <!-- Define some output appenders -->
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="log\\oper-log.txt" />
      <param name="AppendToFile" value="true" />
      <param name="datePattern" value="yyyy-MM-dd HH:mm:ss" />
      <param name="MaxSizeRollBackups" value="10" />
      <param name="MaximumFileSize" value="100KB" />
      <param name="RollingStyle" value="Size" />
      <param name="StaticLogFileName" value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n" />
      </layout>
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] &lt;%X{auth}&gt; - %m%n" />
      </layout>
    </appender>
    <!-- Define an output appender (where the logs can go) -->
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender, log4net">
      <file type="log4net.Util.PatternString" value="%Log\%date{yyyMMdd}.log" />
      <param name="datePattern" value="yyyy-MM-dd HH:mm:ss" />
      <param name="AppendToFile" value="true" />
      <layout type="log4net.Layout.PatternLayout, log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p - %m%n" />
      </layout>
    </appender>
    <!-- Setup the root category, add the appenders and set the default level -->
    <root>
      <level value="All" />
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
  <appSettings>
    <add key="DatabaseServer" value="223.4.233.88"/>
    <add key="DatabaseName" value="test"/>
    <add key="DatabaseUser" value="hep"/>
    <add key="DatabasePwd" value="hep"/>
  </appSettings>
  <connectionStrings>
    <!--<add name="HuiErPuConnectionString" connectionString="Data Source=223.4.233.88;Initial Catalog=HuiErPu;Integrated Security=True" providerName="System.Data.SqlClient" />-->
    <!--<add name="HuiErPuConnectionString2" connectionString="Data Source=223.4.233.88;Initial Catalog=HuiErPu;Integrated Security=True" providerName="System.Data.SqlClient" />-->
    <add name="HuiErPuConnectionString" connectionString="Data Source=223.4.233.88;Initial Catalog=test;User ID=hep;Password=hep" providerName="System.Data.SqlClient" />
    <add name="HuiErPuConnectionString2" connectionString="Data Source=223.4.233.88;Initial Catalog=test;User ID=hep;Password=hep" providerName="System.Data.SqlClient" />

  </connectionStrings>
  <system.web>
